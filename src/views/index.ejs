<script>var timer;
   var compareDate = new Date();
   compareDate.setDate(compareDate.getDate() + 7); //just for this demo today + 7 days
   
   timer = setInterval(function() {
     timeBetweenDates(compareDate);
   }, 1000);
   
   function timeBetweenDates(toDate) {
     var dateEntered = toDate;
     var now = new Date();
     var difference = dateEntered.getTime() - now.getTime();
   
     if (difference <= 0) {
   
       // Timer done
       clearInterval(timer);
     
     } else {
       
       var seconds = Math.floor(difference / 1000);
       var minutes = Math.floor(seconds / 60);
       var hours = Math.floor(minutes / 60);
   
       minutes %= 60;
       seconds %= 60;
   
       $("#minutes").text(minutes);
       $("#seconds").text(seconds);
     }
   }

function question_count(id,i,questions){
      questions.forEach((element,index) => {
         $(`#btn_color${index+1}`).removeClass('btn btn-blue').addClass('btn btn-white')
      });
      $(`#btn_color${i}`).removeClass('btn btn-white').addClass('btn btn-blue')
   const res = questions[i-1];
   $("#ques_count").html(`${res.question_name}`);
   $("#option_1").html(`${res.option_1}`);
   $("#option_2").html(`${res.option_2}`);
   $("#option_3").html(`${res.option_3}`);
   $("#option_4").html(`${res.option_4}`);
   $("#question").html(`Question ${i}`)
    
   if(i>1){
      $("#prev").show();
      $( "#prev" ).unbind();
      $( "#prev" ).click(function(){
         prev_data(i,questions);
      })    
   }else{
      $("#prev").hide();
   }     
}

function prev_data(i, questions){
   let id = questions[i-2]._id
   const res = questions[i-2];
   $("#ques_count").html(`${res.question_name}`);
   $("#option_1").html(`${res.option_1}`);
   $("#option_2").html(`${res.option_2}`);
   $("#option_3").html(`${res.option_3}`);
   $("#option_4").html(`${res.option_4}`);
   $("#question").html(`Question ${i-1}`)
    
   if(i>2){
      $("#prev").show();
      $( "#prev" ).unbind();
      $( "#prev" ).click(function(){
         prev_data(i-1,questions);
      })    
   }else{
      $("#prev").hide();
   }
     
}

</script>
